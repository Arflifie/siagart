{
    "version": 2,
    "framework": "vite",
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.2" }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/(assets|css|js|images|img|storage|favicon.ico|robots.txt)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}